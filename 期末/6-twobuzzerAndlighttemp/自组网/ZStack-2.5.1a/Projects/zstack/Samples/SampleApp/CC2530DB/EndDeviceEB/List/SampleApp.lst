    355          static void sendGtwReport(uint16 source, uint16 parent, uint8 temp, uint8 voltage)
    356          {
    357            uint8 pFrame[ZB_RECV_LENGTH];
    358            
    359            // Start of Frame Delimiter
    360            pFrame[FRAME_SOF_OFFSET] = CPT_SOP; // Start of Frame Delimiter
    361            
    362            // Length
    363            pFrame[FRAME_LENGTH_OFFSET] = 10;
    364            
    365            // Command type
    366            pFrame[FRAME_CMD0_OFFSET] = LO_UINT16(ZB_RECEIVE_DATA_INDICATION);   
    367            pFrame[FRAME_CMD1_OFFSET] = HI_UINT16(ZB_RECEIVE_DATA_INDICATION); 
    368            
    369            // Source address
    370            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_SRC_OFFSET] = LO_UINT16(source); 
    371            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_SRC_OFFSET+ 1] = HI_UINT16(source);
    372            
    373            // Command ID
    374            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_CMD_OFFSET] = LO_UINT16(SAMPLEAPP_P2P_CLUSTERID); 
    375            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_CMD_OFFSET+ 1] = HI_UINT16(SAMPLEAPP_P2P_CLUSTERID);
    376            
    377            // Length
    378            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_LEN_OFFSET] = LO_UINT16(4); 
    379            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_LEN_OFFSET+ 1] = HI_UINT16(4);
    380            
    381            // Data
    382            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_DATA_OFFSET] = temp;
    383            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_DATA_OFFSET+ 1] = voltage; 
    384            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_DATA_OFFSET+ 2] = LO_UINT16(parent); 
    385            pFrame[FRAME_DATA_OFFSET+ ZB_RECV_DATA_OFFSET+ 3] = HI_UINT16(parent);
    386            
    387            // Frame Check Sequence
    388            pFrame[ZB_RECV_LENGTH - 1] = calcFCS(&pFrame[FRAME_LENGTH_OFFSET], (ZB_RECV_LENGTH - 2) );
    389            
    390            // Write report to UART
    391            HalUARTWrite(HAL_UART_PORT_0,pFrame, ZB_RECV_LENGTH);
    392          }
